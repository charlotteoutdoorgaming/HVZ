{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5000"
      },
      "Https": {
        "Url": "https://*:5001"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "path/to/fullchain.pem",
        "KeyPath": "path/to/privkey.pem"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoConfig": {
    "DatabaseName": "playhvz",
    "Host1": "localhost",
    "Port1": 27017
  },
  "ImageServiceOptions": {
    "UploadPath": "wwwroot/images"
  },
  "DiscordIntegrationSettings": {
    "ClientId": "",
    "ClientSecret": ""
  },
  "EmailServiceOptions": {
    "SmtpHost": "smtp.gmail.com",
    "Port": 587
  },
  "WebConfig": {
    "DomainName": "https://www.playhvz.org"
  },
  "AllowedHosts": "*"
  
}

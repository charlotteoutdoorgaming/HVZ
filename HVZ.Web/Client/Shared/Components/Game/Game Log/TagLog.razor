@inherits GameLogBase

<EventLogMessage
    TimeStamp="@GameLog.Timestamp"
    Title="@Title"
    Users="@(new[] {
        GameLog.User,
        ((TagEventLogData)GameLog).TagReceiver
    })" />

@code {
    string Title => $"{GameLog.User.FullName} has tagged {((TagEventLogData)GameLog).TagReceiver.FullName}";
}
